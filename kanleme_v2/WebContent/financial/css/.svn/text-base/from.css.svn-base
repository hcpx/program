.from-box{
    width: 400px;
    margin: 0 auto;
}

.input-group {
    height: 77px;
}

.input-field {
    font-size: 16px;
    color: #CFCFCF;
}

.input-field label {
    font-size: 16px;
    width: 9%;
    text-align: center;
    display: inline-block;
    color: #000;
}

.input-field .text-input {
    height:30px; line-height:30px;
    border: 1px solid #F2F2F2;
    padding:0 10px;
    margin-left: 22px;
    border-radius: 5px;
    background:#eeeeee;
}

.input-area-group .text-area {
    padding: 8px 4px;
    border: 1px solid #F2F2F2;
    width: 78%;
    margin-left: 22px;
    border-radius: 5px;
}

.input-field .text-select
{
    padding: 8px 4px;
    border: 1px solid #F2F2F2;
    width: 68%;
    margin-left: 22px;
    border-radius: 5px;
}

.from-submit {
    margin-top: 0px;
    padding-left: 124px;
}

.from-submit button {
    font-size: 16px;
    color: #fff;
    background: #1ab394;
    border: none;
    border-radius: 5px;
    width: 100%;
    padding: 15px 0;
}

.from-submit-tow button{
    width: auto;
    padding: 15px 42px;
}

.from-submit .submit-btn
{
}

.from-box .b-l{
    float: left;
}

.from-box .b-r{
    float: right;
}

.from-box .cancel-btn{
    color: #000;
    border: 1px solid #E6EAEE;
    background: #FEFEFE;
}

.error-notic {
    color: #ED2655;
    padding: 4px 0 8px;
    display: none;
}