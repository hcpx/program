line-height: 22px\9; /* for ie */ }
.change_mobile ul li:nth-of-type(3) input,.change_mobile_1 ul li:nth-of-type(3) input{ width:100%; background:#f65700; height:40px; line-height:40px;-webkit-border-radius: 5px; text-align:center; color:#FFFFFF;}
.change_mobile_1 ul li:nth-of-type(1) span{ float:left; width:40%;height:40px; line-height:40px;}
.change_mobile_1 ul li:nth-of-type(1) b{ float:right; width:60%}
.change_mobile_1 ul li:nth-of-type(1) b input{-webkit-box-sizing:border-box; width:100%; height:40px; line-height:40px; border:1px solid #dddddd;-webkit-border-radius: 5px;padding-left:10px;line-height: normal; /* for non-ie */ 
line-height: 22px\9; /* for ie */ }
.change_mobile_1 ul li:nth-of-type(2) span{float:left; width:55%;height:40px; line-height:40px;}
.change_mobile_1 ul li:nth-of-type(2) span input{  -webkit-box-sizing:border-box;  width:100%; height:40px; line-height:40px; border:1px solid #dddddd;-webkit-border-radius: 5px; padding-left:10px;
line-height: normal; /* for non-ie */ 
line-height: 22px\9; /* for ie */ }
.change_mobile_1 ul li:nth-of-type(2) b{ float:right; width:30%;}
.change_mobile_1 ul li:nth-of-type(2) b input{ width:100%;  height:40px; line-height:40px; text-align:center; background:-webkit-linear-gradient(top, #f65700,#f65700);-webkit-border-radius: 5px; color:#ffffff;}
/*个人明细*/
.my_task_details{-webkit-box-sizing:border-box; background:#ffffff; margin:10px auto 0 auto; width:100%; padding:0 5px 10px 5px;}
.my_task_details dt{ font-size:18px; color:#bcbcbc; padding-bottom:10px;}
.my_task_details dd,.my_task_details li b,.my_task_details li span,.my_task_details li strong{ float:left; text-align:center; }
