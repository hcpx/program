@charset "utf-8";
/* CSS Document */

body{ max-width:640px; margin:0 auto;  padding:0px; min-width:320px; font-size:14px; background:#eeeeee;-webkit-tap-highlight-color:rgba(0,0,0,0);}
.f_orange{ color:#f65700;}
*{ padding:0px; margin:0px;}
.fl{ float:left;}
.ff{ float:right;}
img{   -webkit-transform: scale(1) rotate(0) translate3d(0,0,0); transform: scale(1) rotate(0) translate3d(0,0,0);}
a:link{color:#000000;}   
a:active{color:#000000;}   
a:visited{color:#000000;}   
a:hover{color:#000000;} 
input[type="button"], input[type="submit"], input[type="reset"],input[type="text"],input[type="tel"] {-webkit-appearance: none;}
/*会员中心通用顶部*/
header{ -webkit-box-sizing:border-box; height:49px; background:rgba(255,255,255,1); margin-bottom:10px; max-width:640px; width:100%;  padding:5px 5px; z-index:9; width:100%; max-width:640px;}
header .home{ line-height:35px; margin-top:2px; width:80px; background:url(../images/app_logo.png) no-repeat left; width:150px; height:35px; background-size: auto 35px; }
header .user{ background:url(../images/icon.png) top center no-repeat; width:24px; height:24px; background-size:24px auto;}
.no_re{ margin-top:7px; color: #FFFFFF;  height:26px; line-height:26px; width:50px; display: block; float:right; margin-left:10px; border:1px solid #d65801; border-radius:5px; color:#ffffff; text-align:center; background:-webkit-gradient(linear,0 0,0 100%,from(#fb8839),to(#f26401)); background:-ms-gradient(linear,0 0,0 100%,from(#fb8839),to(#f26401)); background:-o-gradient(linear,0 0,0 100%,from(#fb8839),to(#f26401)); background:-moz-gradient(linear,0 0,0 100%,from(#fb8839),to(#f26401)); background:gradient(linear,0 0,0 100%,from(#fb8839),to(#f26401));}
.user{margin-top:10px;}
.no_re:visited{ color:#FFFFFF;}
.no_re:link{ color:#FFFFFF;} 
/*首页个人信息*/
.ainfo{ padding:10px 0 10px 0; clear:both; position:relative; background:#FFFFFF;}
.ainfo dt{ position:absolute; top:8px; left:10px;}
.ainfo dt img{border-radius:50px; width:80px; height:80px;-webkit-box-sizing:border-box; }
.ainfo dd{  height:22px; line-height:22px; margin:0px 0px 0px 95px; padding:0 5px; background-color:#fff;}
.ainfo dd:nth-of-type(1){ background-position:5px -35px;}
.ainfo dd:nth-of-type(2){ background-position:5px -88px;}
.ainfo dd:nth-of-type(2) a{ font-size:12px; padding:3px 6px; height:20px; line-height:20px; color:#fff; border-radius:10px; background:#3cb160; width:60px; display:inline;}

.ainfo dd:nth-of-type(1) .logout{ width:50px; height:40px; line-height:40px; background:#f65700; color:#fff;}

.ubutton{ clear:both;}
.ubutton dd{ background:#ffffff url(../images/user_top.png) no-repeat right; background-size:20px auto; margin-top:10px;}
.ubutton dd a{ height:40px; line-height:40px;  color:#2e2e2e; background-image:url(../images/icon.png); background-repeat:no-repeat; background-size:30px auto; text-indent:40px;}
.ubutton dd:nth-of-type(1) a{ background-position:5px -213px;}
.ubutton dd:nth-of-type(2) a{ background-position:5px -275px;}
.ubutton dd:nth-of-type(3) a{ background-position:5px -342px;}
.ubutton dd:nth-of-type(4) a{ background-position:5px -690px; background-size:35px auto;}
.ubutton dd:nth-of-type(5) a{ background-position:5px -735px; background-size:35px auto;}
.ubutton dd:nth-of-type(6) a{ background-position:5px -542px;}
.get_user_out{ margin:10px 10px 0 10px; background: #f65700;height: 40px;line-height: 40px;color: #fff;border: none;border-radius: 5px;}
.get_user_out a{ text-align:center; color:#FFFFFF;}
/*任务列表*/
.task dl{ padding:5px 0px; clear:both; position:relative;}
.task dl dt{height:120px; overflow: hidden;  width:90px; position:absolute; left:0px; top:5px;}
.task dl dt img{ width:90px;overflow:hidden;}
.task dl dd:nth-of-type(1){ border-bottom:3px solid #FFFFFF; font-size:14px;}
.task dl dd:nth-of-type(1) a{ font-size:14px;}
.task dl dd{ padding:0 0 3px 100px; width:100%; -webkit-box-sizing:border-box; font-size:12px;}
.task li{ margin:0 0px 10px 0px; color:#a1a1a1;  background:#ffffff; padding:8px 10px;}
.task li:last-child{ border-bottom:none;}
.task li h3{ border-bottom:1px dashed #CCCCCC; padding:5px 5px;  font-size:16px; color:#000; background:#fff;}
.task li .data{ height:24px; line-height:24px; padding:2px 0;}
.task li .data:nth-of-type(1){ margin-top:5px; background:url(../images/icon.png) -2px -337px no-repeat; background-size:25px auto; padding-left:26px;}
.task li .data img{ vertical-align:middle; padding-right:4px; margin-top:-3px;}
.task li .data div.fr{ width:50%;}
.task li .data:nth-of-type(2){ background:url(../images/icon.png) -2px -337px no-repeat; background-size:25px auto; padding-left:26px;}
.task li .button{ font-size:12px; padding:3px 6px; height:20px; line-height:20px; color:#fff; border-radius:10px; background:#3cb160; width:60px;}
/*申请提现*/
.apply{ padding:10px 10px; background:#FFFFFF;}
.apply dl{ background:#FFFFFF;padding:5px 0; height:60px; line-height:60px; clear:both; position:relative;}
.apply dt{ float:left; width:60px; height:60px; position:absolute; top:0px; left:0px;}
.apply dt img{ width:60px; height:60px;-webkit-border-radius: 30px;}
.apply dd{padding-left:10px; margin-left:55px;-webkit-box-sizing:border-box;}

.apply h2{clear:both; }
.apply .total strong{ font-size:12px; font-weight:normal;} 
.apply .total{background:url(../images/img1.png) right bottom no-repeat #f4ddc3; height:80px; line-height:80px; text-indent:10px; font-size:18px; font-weight:bold; color:#fd3615; background-size:auto 70px;}
.apply form{ padding:10px 0px; color:#000;}
.apply form div{}
.apply form span{color:#ff1a1a; font-weight:bold; padding-right:2px;}
.apply form input{-webkit-box-sizing:border-box; height:40px; line-height:40px; border:1px solid #dddddd; width:100%; text-indent:6px;}
.apply form input:focus{-webkit-box-sizing:border-box; border:1px solid #ff7a23;}
.apply form .code{width:100%;}
.apply form .codes{background:#ededed; width: auto; position:absolute; right:0px; top:0px; padding:0 10px;}
.apply001{margin-top:10px; padding-bottom:2px;}
.apply form input[type=submit]{ width:100%; background:#f65700;  border-radius: 5px; border:none; margin-top:5px;  height:40px; line-height:40px; color:#fff; }
/*首页*/
.header_div{ z-index: auto; position:relative; }
.header_div001{ position:absolute; top:5px; right:5px;z-index:8;}
.header_div002{ width:100%; }
.header_div002 img{ width:100%; vertical-align:middle;}
#con{ width:100%; margin: 0 auto;}
#tags { background: #ffffff;  height:40px;}
#tags LI {float:left; width:33.3%;  text-align: center;padding-top:10px;}
#tags LI A { padding:0 0px; display:inherit;   COLOR: #2e2e2e; font-size:16px;}
#tags LI.selectTag A { display: inline; padding:0 10px;   COLOR: #f65700 ;border-bottom:3px solid #f65700; font-size:16px; padding-bottom:8px;}
.tagContent { DISPLAY: none; clear:both; }
#tagContent DIV.selectTag {DISPLAY: block}
.tab_show_cash{ padding-top:10px;  margin:0 auto;}
.ad_show{ background: #ffffff;margin:0 0px 10px 0px; padding-bottom:5px; overflow:hidden; height:1%;}
.ad_show_div{  color:#000000; font-size:16px; padding:10px 0 0px 10px;}
.ad_show_div strong{ float:left;}
.ad_show_ads { margin:5px 10px; position:relative; max-height:200px; overflow: hidden;}
.ad_show_title{padding:8px 8px 8px 5px;-webkit-box-sizing:border-box; position:absolute; bottom:0px; background:rgba(0,0,0,0.5); width:100%;font-size: 1.0em;}
.ad_show_title a{ color:rgba(255,255,255,0.6);}
.ad_show_ads img{ width:100%; vertical-align:top; }
.ad_show_click{ display: none; color:#afaeae; clear:both;  overflow: hidden;  height:1%;-webkit-box-sizing:border-box;}
.ad_show_click strong{ color: #f65700;}
.ad_show_click001{ float:left;}
.ad_show_click002{ float:right;text-align: right;}
.ad_show_click002 strong{ color:#f65700;}
.tab_show_top{ margin: 0 auto;  padding-top:10px;}
.tab_show_top li { width:100%; clear:both; margin:0 auto; padding-bottom:10px;overflow: hidden; height:1%;}
.tab_show_top li.tab_show_top001{ background:#ffffff; padding:0px 10px; height:70px; line-height:70px;-webkit-box-sizing:border-box; margin:0 auto 10px auto;}
.tab_show_top li dl{ color:#f65700; padding-bottom:15px; font-size:16px;}
.tab_show_top li dt{ float:left; width:50%; text-align:center;}
.tab_show_top li dd{ float:right; width:50%; text-align: center; }
.tab_show_top li span{ float:left; width:50%; padding-left:30px;-webkit-box-sizing:border-box; text-align:left; color:#000000; }
.tab_show_top li b{ float:right; width:50%; text-align:center; color:#b8b8b8; font-size:14px;}
.tab_show_top li span img{-webkit-border-radius: 30px;}
.tab_show_top li b strong{ color:#ff8125; }
.tab_show_show{ margin: 0 auto; padding-top:10px;}
.tab_show_show li { width:100%; clear:both; margin:0 auto; padding-bottom:12px;overflow: hidden; height:1%;}
.tab_show_show li dl{ color:#f65700; padding-bottom:10px; font-size:16px;}
.tab_show_show li dt{ text-indent:10px; float:left; width:40%; -webkit-box-sizing:border-box;  text-align:left; display:block; }
.tab_show_show li dd{ float:right; width:30%; display: block; padding-left:5px;-webkit-box-sizing:border-box; }
.tab_show_show li.tab_show_show001{background:#ffffff;-webkit-box-sizing:border-box; padding:0px 10px; height:70px; line-height:70px; margin:0 auto 10px auto;}
.tab_show_show li.tab_show_show001 span{ float:left; width:50%; padding-left:30px;-webkit-box-sizing:border-box; font-weight: normal; display:block;}
.tab_show_show li.tab_show_show001 span img{-webkit-border-radius: 30px;}
.tab_show_show li.tab_show_show001 a{ float:right; text-align:center; width:50%; display: block;  color:#d7d7d7; }
.tab_show_show li.tab_show_show001 a{color:#ff8125; d }
/*首页动画效果*/
.tpbtn{ position:absolute; z-index:99999;left:0px; bottom:0px;opacity:0.8;}
.tpbtn div{ -webkit-transform: scale(1) rotate(0) translate3d(0,0,0); transform: scale(1) rotate(0) translate3d(0,0,0); background:url(../images/tpbtn.png) no-repeat; width:60px; height:60px;display:block; background-size:100%;-webkit-tap-highlight-color:#fff000); z-index:9999;}
.kjz_app{position: fixed; left:20px; bottom:28px;}
.show_app_nones{ display:none;}
.show_app_none{
-webkit-animation: show_app_nones 0.3s linear; -webkit-animation-fill-mode:forwards; 
animation: show_app_nones 0.3s linear; animation-fill-mode:forwards; 
-moz-animation: show_app_nones 0.3s linear; -moz-animation-fill-mode:forwards; 
-o-animation: show_app_nones 0.3s linear; -o-animation-fill-mode:forwards; 
 background:url(../images/show_apps.png) no-repeat; width:147px; height:147px;display:block;background-size:147px auto ; }
@keyframes show_app_nones{0%{transform:translate(60px,0px) ;opacity:0.8}100%{transform:translate(-160px,120px);opacity:1;}}
@-webkit-keyframes show_app_nones{0%{-webkit-transform:translate(60px,0px) ;opacity:0.8}100%{-webkit-transform:translate(-180px,160px);opacity:1;}}
.show_app_none li{  position:absolute;}
.show_app_none li a{ display:block;width:30px; height:30px;}
.show_app_none li:nth-of-type(1){ left:10px; top:55px;}
.show_app_none li:nth-of-type(2){ left:55px; top:55px; }
.show_app_none li:nth-of-type(3){ right:10px; top:55px;}
.show_app { position:absolute; z-index:9999999; left:60px ; bottom:0px; background:url(../images/show_apps.png) no-repeat; width:147px; height:148px;display:block;background-size:147px 148px; ;-webkit-animation: huan-rotate-1 0.3s linear ;}
@keyframes huan-rotate-1{0%{-webkit-transform:translate(-160px,120px) ;opacity:0.0}100%{-webkit-transform:translate(0px,0px);opacity:1}}
@-webkit-keyframes huan-rotate-1{0%{-webkit-transform:translate(-210px,120px) ;opacity:0.0}100%{-webkit-transform:translate(0px,0px);opacity:1}}
@-o-keyframes huan-rotate-1{0%{-o-transform:translate(-210px,120px) ;opacity:0.0}100%{-o-transform:translate(0px,0px);opacity:1}}
.show_app li{  position:absolute; }
.show_app li a{ display:block;width:30px; height:30px;}
.show_app li:nth-of-type(1){ left:10px; top:55px;}
.show_app li:nth-of-type(2){ left:55px; top:55px; }
.show_app li:nth-of-type(3){ right:10px; top:55px;}
.page_index{ width: 100%;height: 100%;position: fixed; top: 0;left: 0;background: #000000 ;opacity: 0.5;-webkit-transform: translate3d(0,0,0);}
.page_index_none{ display:none;}
/*预览*/
.goods_onlick{ position:fixed; top:0px;z-index:10; max-width:640px; width:100%; }
.goods_onlick_ico{ position:relative;}
.goods_onlick_ico001 a{ z-index:999999; position:absolute; right:50px; top:50px; background:url(../images/ico_pre.png) no-repeat center; width:50px; height:50px;display:block; background-size:50px  auto;}

.goods_onlick_ico002 a{box-shadow: #000000 0px 0px 5px; font-weight:normal; background:rgba(0,0,0,0.7); z-index:9999; color:#FFFFFF; text-align: center; line-height:38px; position:absolute; right:5px; top:55px;border-radius: 30px; border:1px solid #FFFFFF; width:38px; height:38px;display:block;}
.goods_show{width:100%; padding:0 0px; -webkit-box-sizing:border-box; margin: 0 auto 0px auto;}
.goods_show_img{  overflow: hidden; background:#FFFFFF;box-sizing:border-box;}
.goods_show_img img{ width:100%; vertical-align:top;}
.goods_show h1{ background:#FFFFFF; font-size:14px; padding:10px 0 2px 0; padding-left:10px; }
.goods_show_dec{ margin:0 10px;  padding:0px 0px 0px 0px; color:#999999;background:#FFFFFF; line-height:22px;}
.goods_show_dec a{ font-size:12px; background:#8e8e8e; color:#FFFFFF; text-align:center; padding:4px 5px; text-decoration:none;border-radius: 10px;}
.goods_show .goods_show_tile{ font-size:14px; padding-bottom:10px; overflow: hidden; height:1%; color:#bababa;}
.goods_show .goods_show_tile span,.goods_show .goods_show_tile b{  float:left;}
.goods_show .goods_show_tile b{text-indent:10px;}
.goods_show_img{ width:100%; clear:both;}
.goods_show_img img{ max-width:100%;}
.good_dec{ width:95%; margin:0 auto;}
.read_more{ text-align:right;  padding-top:10px;}
.read_more a{ color:#f65700;}
.goods_condition{ padding-top:5px; background:#FFFFFF;}
.goods_condition ul{overflow:hidden; height:1%; background:#FFFFFF;}
.goods_condition li{ color:#f65700; background:#ffffff;  margin:0 10px 0px 10px; line-height:24px;}
.goods_condition li:nth-of-type(3){ border-bottom:none;}
.goods_condition li strong{ color:#000000;}
.submit_order{ display: block; background:rgba(252,252,252,0.95); position:fixed; z-index:10; bottom:0px; max-width:640px; width:100%; height:75px; overflow:hidden; text-align:center; padding:10px ;-webkit-box-sizing:border-box; border-top:1px solid #e7e7e7;}

.submit_order_xie{ margin-top:5px;}
.submit_order_xie input{}
.submit_order_xie b {position: relative; width:auto;}
.submit_order_xie b label {cursor: pointer;position: absolute;width: 14px;height: 14px;left: -2px; top:1px;border-radius:3px;border:1px solid #ff591a; background:#FFFFFF;}
.submit_order_xie b label:after {opacity: 0;content: '';position: absolute;width: 7px;height: 2px;background: transparent;top: 4px;left: 3px;border: 2px solid #ff591a;border-top: none;border-right: none;-webkit-transform: rotate(-45deg);transform: rotate(-45deg);}
.submit_order_xie b label:hover::after {opacity: 0.0;}
.submit_order_xie b input[type=checkbox]:checked + label:after {opacity: 1;}
.submit_order_xie input{ display:none; background: none; color:#FFFFFF; border:1px solid #FFFFFF;}
 
.submit_get{ padding:0 20px; height:40px; display:inline; color:#f65700; text-align: center; line-height:40px; font-size:18px; text-decoration:underline; color:#f65700;}
.submit_order a:link{ color:none;}
.submit_share{ color:#FFFFFF; display:inline; padding:10px 40px; height:40px;  background:#f65700; text-align:center; line-height:40px; font-size:18px;-webkit-border-radius: 8px;}

.recipients{ clear:both; padding-top:10px;}
.recipients ul{ background:#FFFFFF; padding-bottom:10px;}
.recipients li:nth-of-type(1){ height:40px; line-height:40px;}
.recipients li{ background:#FFFFFF; border-bottom:1px dashed #d9d9d9 ; height:70px; line-height:70px; overflow:hidden; height:1%; margin:0 10px 0 10px; }

.recipients li span{ float:left; width:50%; text-align:left;text-indent:10px; color: #000000;}
.recipients li span img{-webkit-border-radius:30px;}
.recipients li b{ float:left; width:47%; text-align:right; color: #000000;}
.recipients001{ color: #CCCCCC; }
/*个人信息*/
.my_information{ margin:10px auto 0 auto; width:100%;-webkit-box-sizing:border-box; padding:0 5px;}
.my_information li {color:#666666; margin-bottom:10px; height:40px; line-height:40px; 
background-color: #fff;
background-image: url(../images/icon.png);background-repeat: no-repeat;background-size: 25px;}
.my_information li input{ -webkit-box-sizing:border-box; padding:0 5px; width:80%; margin-left:5px; border:1px solid #FFFFFF; border-radius:5px;}
.my_information li input.onfocus{-webkit-box-sizing:border-box; border:1px solid #ff7a23;}
.my_information li:nth-of-type(2){background-position: 5px -372px;}
.my_information li:nth-of-type(1){background-position: 5px -35px;}
.my_information li:nth-of-type(3){ background: #FFFFFF;height:inherit; line-height:40px; padding-bottom:20px;}
.my_information li span{ text-indent:38px; float:left; width:70%; color:#666666; }
.my_information li  i{ font-weight: normal; color:#f65700;}
.my_information li b{width:20%; float:right; background:#f65700; text-align:center; color:#FFFFFF;}
.my_information li b a{ color:#FFFFFF; text-align:center;}
.my_information li dt{ height:40px; background-image: url(../images/icon.png);background-repeat: no-repeat;background-position: 5px -85px;background-size: 25px;  text-indent:38px; }
.my_information li dl dd:nth-of-type(1){ padding-top:10px;}
.my_information li dl dd{ line-height:30px; padding-left:30px; margin-bottom:5px; color:#bababa;}
.my_information li dl dd:nth-of-type(4){  background:#f65700;height:40px; line-height:40px; margin:10px 10px 0 10px; text-align:center; padding-left:inherit; border-radius:5px;}
.my_information li dl dd:nth-of-type(4) a{color:#FFFFFF;}
/*修改手机*/
.change_mobile,.change_mobile_1{ margin:20px auto 0 auto; width:100%;-webkit-box-sizing:border-box; padding:0 5px;}
.change_mobile ul li,.change_mobile_1 ul li{ clear:both; padding-bottom:20px; overflow:hidden; }
.change_mobile ul li:nth-of-type(1) span{ float:left; width:70%;height:40px; line-height:40px;}
.change_mobile ul li:nth-of-type(1) b{ float:right; width:30%}
