.from-box{
    width: 400px;
    margin: 0 auto;
}

.input-group {
    height: 77px;
}

.input-field {
    font-size: 16px;
    color: #CFCFCF;
}

.input-field label {
    font-size: 16px;
    width: 12%;
    text-align: right;
    display: inline-block;
    color: #aaa;
}

.input-field .text-input {
    padding: 8px 4px;
    border: 1px solid #F2F2F2;
    width: 78%;
    margin-left: 22px;
    border-radius: 5px;
}

.input-area-group .text-area {
    padding: 8px 4px;
    border: 1px solid #F2F2F2;
    width: 78%;
    margin-left: 22px;
    border-radius: 5px;
}

.input-field .text-select
{
    padding: 8px 4px;
    border: 1px solid #F2F2F2;
    width: 68%;
    margin-left: 22px;
    border-radius: 5px;
}

.from-submit {
    margin-top: 7px;
    padding-left: 75px;
}

.from-submit button {
    font-size: 18px;
    color: #fff;
    background: #20bfa1;
    border: none;
    border-radius: 5px;
    width: 100%;
    padding: 15px 0;
}

.from-submit-tow button{
    width: auto;
    padding: 10px 20px;
}

.from-submit .submit-btn
{
}

.from-box .b-l{
    float: left;
}

.from-box .b-r{
    float: right;
}

.from-box .cancel-btn{
    color: #C3C3C3;
    border: 1px solid #E6EAEE;
    background: #FEFEFE;
}

.error-notic {
    color: #ED2655;
    padding: 4px 0 8px;
    display: none;
}
.actin_top{ width:100%; height:96px; background:#fff; overflow:hidden;}
.actin_top h2{ font-size:24px; height:30px; line-height:30px; margin-left:20px; font-weight:400; margin-top:20px;}
.actin_top p{ font-size:16px; margin-left:20px; height:24px; line-height:24px; margin-top:5px;}